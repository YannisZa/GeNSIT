{
  "sweep_mode": {
    "sweepable": false,
    "optional": false,
    "dtype": "bool",
    "default-value": false
  },
  "log_level": {
    "sweepable": false,
    "optional": false,
    "dtype": "str",
    "default-value": "info",
    "is-any-of": ["info", "warn", "error", "critical", "debug"]
  },
  "inputs": {
    "seed": {
      "sweepable": true,
      "optional": true,

      "sweep": {
        "default": {
          "dtype": "int",
          "default-value": 1234,
          "valid-range": [1, null]
        },
        "range": {
          "dtype": "customList",
          "list-dtype": "int",
          "default-value": "1:100:1",
          "valid-range": [1, null]
        }
      }
    },
    "n_workers": {
      "sweepable": false,
      "optional": true,
      "dtype": "int",
      "default-value": 1,
      "valid-range": [1, 16]
    },
    "n_threads": {
      "sweepable": false,
      "optional": true,
      "dtype": "list",
      "list-length": 2,
      "list-dtype": "int",
      "default-value": [8, 8],
      "valid-range": [1, 16]
    },
    "device": {
      "sweepable": false,
      "optional": true,
      "dtype": "str",
      "default-value": "cpu",
      "is-any-of": ["cpu", "mps", "cuda"]
    },
    "dataset": {
      "sweepable": false,
      "optional": false,
      "dtype": "directory",
      "directory-exists": true,
      "default-value": "./data/inputs/synthetic_2x3_N_100/"
    },
    "load_experiment": {
      "sweepable": false,
      "optional": true,
      "dtype": "str",
      "default-value": ""
    },
    "dims": {
      "sweepable": false,
      "optional": true,
      "dtype": "list",
      "list-length": 2,
      "list-dtype": "int",
      "default-value": [1, 1],
      "valid-range": [1, null]
    },
    "data_files": {
      "sweepable": false,
      "optional": false,

      "origin_demand": {
        "sweepable": true,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "file",
            "file-exists": false,
            "file-extension": "txt",
            "default-value": "origin_demand.txt"
          },
          "range": {
            "dtype": "list",
            "list-length": [1, null],
            "list-dtype": "file",
            "file-exists": false,
            "file-extension": "txt",
            "default-value": ["origin_demand.txt"]
          }
        }
      },
      "destination_attraction_ts": {
        "sweepable": true,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "file",
            "file-exists": false,
            "file-extension": "txt",
            "default-value": "destination_attraction_ts.txt"
          },
          "range": {
            "dtype": "list",
            "list-length": [1, null],
            "list-dtype": "file",
            "file-exists": false,
            "file-extension": "txt",
            "default-value": ["destination_attraction_ts.txt"]
          }
        }
      },
      "cost_matrix": {
        "sweepable": true,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "file",
            "file-exists": false,
            "file-extension": "txt",
            "default-value": "cost_matrix.txt"
          },
          "range": {
            "dtype": "list",
            "list-length": [1, null],
            "list-dtype": "file",
            "file-exists": false,
            "file-extension": "txt",
            "default-value": ["cost_matrix.txt"]
          }
        }
      }
    }
  },
  "spatial_interaction_model": {
    "sweepable": false,
    "optional": true,

    "sim_type": {
      "sweepable": true,
      "optional": false,

      "sweep": {
        "default": {
          "dtype": "str",
          "default-value": "TotallyConstrained",
          "is-any-of": ["TotallyConstrained", "ProductionConstrained"]
        },
        "range": {
          "dtype": "list",
          "list-length": [1, 2],
          "list-dtype": "str",
          "default-value": ["TotallyConstrained"],
          "is-any-of": ["TotallyConstrained", "ProductionConstrained"]
        }
      }
    },
    "grand_total": {
      "sweepable": false,
      "optional": true,
      "dtype": "int",
      "default-value": 1,
      "valid-range": [1, null]
    },
    "parameters": {
      "sweepable": false,
      "optional": false,

      "alpha": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 1.0,
            "valid-range": [0.0, null]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [1.0],
            "valid-range": [0.0, null]
          }
        }
      },
      "beta": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 0.0,
            "valid-range": [0.0, null]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.0],
            "valid-range": [0.0, null]
          }
        }
      },
      "bmax": {
        "sweepable": true,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 1.0,
            "valid-range": [0.0, null],
            "inclusive": false
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.0],
            "valid-range": [0.0, null],
            "inclusive": false
          }
        }
      }
    }
  },
  "harris_wilson_model": {
    "sweepable": false,
    "optional": false,

    "dt": {
      "sweepable": true,
      "optional": false,

      "sweep": {
        "default": {
          "dtype": "float",
          "default-value": 0.001,
          "valid-range": [0, 1]
        },
        "range": {
          "dtype": "list",
          "list-dtype": "float",
          "default-value": [0.001],
          "valid-range": [0, 1]
        }
      }
    },
    "parameters": {
      "sweepable": false,
      "optional": false,

      "delta": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 0.0,
            "valid-range": [0.0, null]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.0],
            "valid-range": [0.0, null]
          }
        }
      },
      "kappa": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 0.0,
            "valid-range": [0.0, null],
            "inclusive": false
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.0],
            "valid-range": [0.0, null],
            "inclusive": false
          }
        }
      },
      "sigma": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 0.0,
            "valid-range": [0.0, null]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.0],
            "valid-range": [0.0, null]
          }
        }
      },
      "epsilon": {
        "sweepable": true,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 1.0,
            "valid-range": [0.0, null],
            "inclusive": false
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.0],
            "valid-range": [0.0, null],
            "inclusive": false
          }
        }
      }
    }
  },
  "neural_network": {
    "sweepable": false,
    "optional": false,

    "disable_tqdm": {
      "sweepable": false,
      "optional": true,
      "dtype": "bool",
      "default-value": true
    },
    "table_inference": {
      "sweepable": false,
      "optional": false,
      "dtype": "bool",
      "default-value": false
    },
    "N": {
      "sweepable": true,
      "optional": false,

      "sweep": {
        "default": {
          "dtype": "int",
          "default-value": 1,
          "valid-range": [1, null]
        },
        "range": {
          "dtype": "list",
          "list-dtype": "int",
          "list-length": [1, null],
          "default-value": [1],
          "valid-range": [1, null]
        }
      }
    },
    "num_steps": {
      "sweepable": false,
      "optional": true,
      "dtype": "int",
      "default-value": 1,
      "valid-range": [1, null]
    },
    "batch_size": {
      "sweepable": false,
      "optional": false,
      "dtype": "int",
      "default-value": 1,
      "valid-range": [1, null]
    },
    "to_learn": {
      "sweepable": true,
      "optional": false,

      "sweep": {
        "default": {
          "dtype": "list",
          "list-dtype": "str",
          "default-value": ["alpha", "beta", "sigma"],
          "is-any-of": ["alpha", "beta", "kappa", "delta", "sigma"]
        },
        "range": {
          "dtype": "list",
          "list-dtype": "list",
          "list-list-dtype": "str",
          "default-value": [["alpha", "beta", "sigma"]],
          "is-any-of": ["alpha", "beta", "kappa", "delta", "sigma"]
        }
      }
    },
    "loss_function": {
      "sweepable": false,
      "optional": true,

      "name": {
        "sweepable": false,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "str",
            "default-value": "mseloss",
            "is-any-of": [
              "l1loss",
              "mseloss",
              "crossentropyloss",
              "ctcloss",
              "nllloss",
              "poissonnllloss",
              "gaussiannllloss",
              "kldivloss",
              "bceloss",
              "bcewithlogitsloss",
              "marginrankingloss",
              "hingeembeddingloss",
              "multilabelmarginloss",
              "huberloss",
              "smoothl1loss",
              "softmarginloss",
              "multilabelsoftmarginloss",
              "cosineembeddingloss",
              "multimarginloss",
              "tripletmarginloss",
              "tripletmarginwithdistanceloss"
            ]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "str",
            "default-value": ["mseloss"],
            "is-any-of": [
              "l1loss",
              "mseloss",
              "crossentropyloss",
              "ctcloss",
              "nllloss",
              "poissonnllloss",
              "gaussiannllloss",
              "kldivloss",
              "bceloss",
              "bcewithlogitsloss",
              "marginrankingloss",
              "hingeembeddingloss",
              "multilabelmarginloss",
              "huberloss",
              "smoothl1loss",
              "softmarginloss",
              "multilabelsoftmarginloss",
              "cosineembeddingloss",
              "multimarginloss",
              "tripletmarginloss",
              "tripletmarginwithdistanceloss"
            ]
          }
        }
      }
    },
    "hyperparameters": {
      "sweepable": false,
      "optional": false,

      "num_layers": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "int",
            "default-value": 1,
            "valid-range": [1, null]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "int",
            "default-value": [1],
            "valid-range": [1, null]
          }
        }
      },
      "optimizer": {
        "sweepable": true,
        "optional": true,

        "sweep": {
          "default": {
            "dtype": "str",
            "default-value": "Adam",
            "is-any-of": [
              "Adagrad",
              "Adam",
              "AdamW",
              "SparseAdam",
              "Adamax",
              "ASGD",
              "LBFGS",
              "NAdam",
              "RAdam",
              "RMSprop",
              "Rprop",
              "SGD"
            ]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "str",
            "default-value": ["Adam"],
            "is-any-of": [
              "Adagrad",
              "Adam",
              "AdamW",
              "SparseAdam",
              "Adamax",
              "ASGD",
              "LBFGS",
              "NAdam",
              "RAdam",
              "RMSprop",
              "Rprop",
              "SGD"
            ]
          }
        }
      },
      "learning_rate": {
        "sweepable": true,
        "optional": false,

        "sweep": {
          "default": {
            "dtype": "float",
            "default-value": 0.002,
            "valid-range": [0, 1]
          },
          "range": {
            "dtype": "list",
            "list-dtype": "float",
            "default-value": [0.002],
            "valid-range": [0, 1]
          }
        }
      },
      "biases": {
        "sweepable": false,
        "optional": true,

        "default": {
          "sweepable": false,
          "optional": true,

          "dtype": "list",
          "list-dtype": "float",
          "default-value": [0.0, 4.0],
          "valid-range": [null, null]
        },
        "layer_specific": {
          "sweepable": false,
          "optional": true,

          "dtype": "dict",
          "dict-key-dtype": "str",
          "dict-value-dtype": "float",
          "default-value": {},
          "key-valid-range": [0, null],
          "value-valid-range": [null, null]
        }
      },
      "nodes_per_layer": {
        "sweepable": false,
        "optional": true,

        "default": {
          "sweepable": false,
          "optional": true,

          "dtype": "int",
          "default-value": 20,
          "valid-range": [1, null]
        },
        "layer_specific": {
          "sweepable": false,
          "optional": true,

          "dtype": "dict",
          "dict-key-dtype": "str",
          "dict-value-dtype": "float",
          "default-value": {},
          "key-valid-range": [0, null],
          "value-valid-range": [null, null]
        }
      },
      "activation_funcs": {
        "sweepable": false,
        "optional": true,

        "default": {
          "sweepable": false,
          "optional": true,

          "dtype": "str",
          "default-value": "linear",
          "is-any-of": [
            "abs",
            "celu",
            "cos",
            "cosine",
            "elu",
            "gelu",
            "hardshrink",
            "hardsigmoid",
            "hardswish",
            "hardtanh",
            "leakyrelu",
            "linear",
            "logsigmoid",
            "mish",
            "prelu",
            "relu",
            "rrelu",
            "selu",
            "sigmoid",
            "silu",
            "sin",
            "sine",
            "softplus",
            "softshrink",
            "swish",
            "tanh",
            "tanhshrink",
            "threshold"
          ]
        },
        "layer_specific": {
          "sweepable": false,
          "optional": true,

          "dtype": "dict",
          "dict-key-dtype": "strInt",
          "dict-value-dtype": "str",
          "default-value": { "1": "abs" },
          "key-valid-range": [0, null],
          "value-is-any-of": [
            "abs",
            "celu",
            "cos",
            "cosine",
            "elu",
            "gelu",
            "hardshrink",
            "hardsigmoid",
            "hardswish",
            "hardtanh",
            "leakyrelu",
            "linear",
            "logsigmoid",
            "mish",
            "prelu",
            "relu",
            "rrelu",
            "selu",
            "sigmoid",
            "silu",
            "sin",
            "sine",
            "softplus",
            "softshrink",
            "swish",
            "tanh",
            "tanhshrink",
            "threshold"
          ]
        }
      }
    }
  },
  "experiments": {
    "sweepable": false,
    "optional": false,
    "dtype": "list",
    "list-dtype": "customDict",
    "dict-key-dtype": "str",
    "dict-value-schema": {
      "name": {
        "sweepable": false,
        "optional": false,
        "dtype": "str",
        "default-value": "SIM_NN",
        "is-any-of": [
          "SIM_NN",
          "SIM_MCMC",
          "JointTableSIM_NN",
          "JointTableSIM_MCMC"
        ]
      },
      "comment": {
        "sweepable": false,
        "optional": true,
        "dtype": "str",
        "default-value": ""
      },
      "disable_tqdm": {
        "sweepable": false,
        "optional": true,
        "dtype": "bool",
        "default-value": true
      },
      "print_statements": {
        "sweepable": false,
        "optional": true,
        "dtype": "bool",
        "default-value": false
      },
      "export_samples": {
        "sweepable": false,
        "optional": true,
        "dtype": "bool",
        "default-value": true
      },
      "export_metadata": {
        "sweepable": false,
        "optional": true,
        "dtype": "bool",
        "default-value": true
      },
      "overwrite": {
        "sweepable": false,
        "optional": true,
        "dtype": "bool",
        "default-value": true
      }
    }
  },
  "outputs": {
    "sweepable": false,
    "optional": false,

    "write_start": {
      "sweepable": false,
      "optional": true,

      "dtype": "int",
      "default-value": 1,
      "valid-range": [1, null]
    },
    "write_every": {
      "sweepable": false,
      "optional": true,

      "dtype": "int",
      "default-value": 1,
      "valid-range": [1, null]
    },
    "directory": {
      "sweepable": false,
      "optional": false,

      "dtype": "directory",
      "directory-exists": true,
      "default-value": "./data/outputs/"
    },
    "experiment_title": {
      "optional": true,
      "sweepable": false,

      "dtype": "str",
      "default-value": ""
    }
  }
}
